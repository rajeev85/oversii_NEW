
<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Oversii - Admin DashBoard</title>
    <link rel="shortcut icon" href="~/Oversii_favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/Oversii_favicon.ico" type="image/x-icon">

    <!-- Bootstrap Core CSS -->
    <link href="~/Content/DashBoard/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/Content/DashBoard/admin.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/Content/DashBoard/font-awesome.min.css" rel="stylesheet" type="text/css">



</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href=""><img src="~/img/Oversii_long_logo.png" alt="Logo" /></a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-user"></i> @Context.User.Identity.Name <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="#"><i class="fa fa-fw fa-user"></i> Profile</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-envelope"></i> Inbox</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-gear"></i> Settings</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="@Url.Action("Index", "Home")" class="elements"><i class="fa fa-fw fa-power-off"></i> LogOut</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li>
                        <a href="../Admin/DashBoard"><i class="fa fa-fw fa-dashboard"></i> Dashboard</a>
                    </li>
                    <li>
                        <a href="javascript:;" data-toggle="collapse" data-target="#demo"><i class="fa fa-fw fa-wrench"></i> Admin System <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="demo" class="collapse">
                            <li>
                                <a href="@Url.Action("Index", "property_management")" class="elements"><i class="fa fa-fw fa-building-o"></i>     Property Management Company</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "property")" class="elements"><i class="fa fa-fw fa-home"></i>     Properties</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-fw fa-sitemap"></i>     Subdivisions, Street and House Addresses</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-fw fa-newspaper-o"></i>     Permits</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-fw fa-bar-chart-o"></i>     Reports</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-fw fa-car"></i>     Violations</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-fw fa-gear"></i>     Manage Settings</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>


<div id="page-wrapper">
            <div class="container body-content">

                    @RenderBody()

                </div>
                <!-- /.container-fluid -->

</div>
            <!-- /#page-wrapper -->

        </div>
        <!-- /#wrapper -->
        <!-- jQuery -->
        <script src="~/Scripts/DashBoard/dashboard.min.js"></script>

        <!-- Bootstrap Core JavaScript -->
        <script src="~/Scripts/DashBoard/bootstrap.min.js"></script>






</body>
</html>
